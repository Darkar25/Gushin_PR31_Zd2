<Page x:Class="MaraphonSkills.Pages.LoadVolunteersPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MaraphonSkills.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="LoadVolunteersPage">

    <StackPanel>
        <Label Content="Загрузка волонтеров"/>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Label Content="CSV Файл волонтеров"/>
            <TextBox x:Name="file"/>
            <Button Content="Просмотр..." Click="Button_Click_1"/>
        </StackPanel>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
            <Button Content="Загрузка" Click="Button_Click_2"/>
            <Button Content="Отмена" Click="Button_Click"/>
        </StackPanel>
    </StackPanel>
</Page>
